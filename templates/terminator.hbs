[global_config]
  title_transmit_fg_color = "{{lower primary.bright_white.hex }}"
  title_transmit_bg_color = "{{lower primary.black.hex }}"
  title_inactive_fg_color = "{{lower primary.bright_white.hex }}"
  title_inactive_bg_color = "{{lower primary.bright_black.hex }}"
[profiles]
  [[default]]
    foreground_color = "{{lower primary.bright_white.hex }}"
    background_color = "{{lower primary.black.hex }}"
    cursor_color = "{{lower primary.yellow.hex }}"
    cursor_color_fg = False
    palette = "{{lower primary.black.hex }}:{{lower primary.red.hex }}:{{lower primary.green.hex }}:{{lower primary.yellow.hex }}:{{lower primary.blue.hex }}:{{lower primary.magenta.hex }}:{{lower primary.cyan.hex }}:{{lower primary.white.hex }}:{{lower primary.bright_black.hex }}:{{lower primary.bright_red.hex }}:{{lower primary.bright_green.hex }}:{{lower primary.bright_yellow.hex }}:{{lower primary.bright_blue.hex }}:{{lower primary.bright_magenta.hex }}:{{lower primary.bright_cyan.hex }}:{{lower primary.bright_white.hex }}"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
