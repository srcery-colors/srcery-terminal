[global_config]
  title_transmit_fg_color = "{{lower bright_white.hex }}"
  title_transmit_bg_color = "{{lower black.hex }}"
  title_inactive_fg_color = "{{lower bright_white.hex }}"
  title_inactive_bg_color = "{{lower bright_black.hex }}"
[profiles]
  [[default]]
    foreground_color = "{{lower bright_white.hex }}"
    background_color = "{{lower black.hex }}"
    cursor_color = "{{lower bright_yellow.hex }}"
    cursor_color_fg = False
    palette = "{{lower black.hex }}:{{lower red.hex }}:{{lower green.hex }}:{{lower yellow.hex }}:{{lower blue.hex }}:{{lower magenta.hex }}:{{lower cyan.hex }}:{{lower white.hex }}:{{lower bright_black.hex }}:{{lower bright_red.hex }}:{{lower bright_green.hex }}:{{lower bright_yellow.hex }}:{{lower bright_blue.hex }}:{{lower bright_magenta.hex }}:{{lower bright_cyan.hex }}:{{lower bright_white.hex }}"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
