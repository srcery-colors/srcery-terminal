[global_config]
  title_transmit_fg_color = "{{=c.foreground}}"
  title_transmit_bg_color = "{{=c[0]}}"
  title_inactive_fg_color = "{{=c.foreground}}"
  title_inactive_bg_color = "{{=c[8]}}"
[profiles]
  [[default]]
    foreground_color = "{{=c.foreground}}"
    background_color = "{{=c.background}}"
    cursor_color = "{{=c.cursor}}"
    cursor_color_fg = False
    palette = "{{=c[0]}}:{{=c[1]}}:{{=c[2]}}:{{=c[3]}}:{{=c[4]}}:{{=c[5]}}:{{=c[6]}}:{{=c[7]}}:{{=c[8]}}:{{=c[9]}}:{{=c[10]}}:{{=c[11]}}:{{=c[12]}}:{{=c[13]}}:{{=c[14]}}:{{=c[15]}}"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
