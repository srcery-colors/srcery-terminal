[global_config]
  title_transmit_fg_color = "{{lowerCase primary.bright_white.hex }}"
  title_transmit_bg_color = "{{lowerCase primary.black.hex }}"
  title_inactive_fg_color = "{{lowerCase primary.bright_white.hex }}"
  title_inactive_bg_color = "{{lowerCase primary.bright_black.hex }}"
[profiles]
  [[default]]
    foreground_color = "{{lowerCase primary.bright_white.hex }}"
    background_color = "{{lowerCase primary.black.hex }}"
    cursor_color = "{{lowerCase primary.yellow.hex }}"
    cursor_color_fg = False
    palette = "{{lowerCase primary.black.hex }}:{{lowerCase primary.red.hex }}:{{lowerCase primary.green.hex }}:{{lowerCase primary.yellow.hex }}:{{lowerCase primary.blue.hex }}:{{lowerCase primary.magenta.hex }}:{{lowerCase primary.cyan.hex }}:{{lowerCase primary.white.hex }}:{{lowerCase primary.bright_black.hex }}:{{lowerCase primary.bright_red.hex }}:{{lowerCase primary.bright_green.hex }}:{{lowerCase primary.bright_yellow.hex }}:{{lowerCase primary.bright_blue.hex }}:{{lowerCase primary.bright_magenta.hex }}:{{lowerCase primary.bright_cyan.hex }}:{{lowerCase primary.bright_white.hex }}"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
