[global_config]
  title_transmit_fg_color = "{{ primary.bright_white.hex }}"
  title_transmit_bg_color = "{{ primary.black.hex }}"
  title_inactive_fg_color = "{{ primary.bright_white.hex }}"
  title_inactive_bg_color = "{{ primary.bright_black.hex }}"
[profiles]
  [[default]]
    foreground_color = "{{ primary.bright_white.hex }}"
    background_color = "{{ primary.black.hex }}"
    cursor_color = "{{ primary.yellow.hex }}"
    cursor_color_fg = False
    palette = "{{ primary.black.hex }}:{{ primary.red.hex }}:{{ primary.green.hex }}:{{ primary.yellow.hex }}:{{ primary.blue.hex }}:{{ primary.magenta.hex }}:{{ primary.cyan.hex }}:{{ primary.white.hex }}:{{ primary.bright_black.hex }}:{{ primary.bright_red.hex }}:{{ primary.bright_green.hex }}:{{ primary.bright_yellow.hex }}:{{ primary.bright_blue.hex }}:{{ primary.bright_magenta.hex }}:{{ primary.bright_cyan.hex }}:{{ primary.bright_white.hex }}"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
