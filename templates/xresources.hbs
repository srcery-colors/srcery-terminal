! special
*foreground:   {{lower bright_white.hex }}
*background:   {{lower black.hex }}
*cursorColor:  {{lower bright_yellow.hex }}

! black
*color0:       {{lower black.hex }}
*color8:       {{lower bright_black.hex }}

! red
*color1:       {{lower red.hex }}
*color9:       {{lower bright_red.hex }}

! green
*color2:       {{lower green.hex }}
*color10:      {{lower bright_green.hex }}

! yellow
*color3:       {{lower yellow.hex }}
*color11:      {{lower bright_yellow.hex }}

! blue
*color4:       {{lower blue.hex }}
*color12:      {{lower bright_blue.hex }}

! magenta
*color5:       {{lower magenta.hex }}
*color13:      {{lower bright_magenta.hex }}

! cyan
*color6:       {{lower cyan.hex }}
*color14:      {{lower bright_cyan.hex }}

! white
*color7:       {{lower white.hex }}
*color15:      {{lower bright_white.hex }}
