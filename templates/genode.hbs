<config initial_columns="120" initial_rows="36">
  <!-- srcery color scheme -->
  <color index="0x0" bg="{{ black.hex }}"/>
  <color index="0x1" bg="{{ red.hex }}"/>
  <color index="0x2" bg="{{ green.hex }}"/>
  <color index="0x3" bg="{{ yellow.hex }}"/>
  <color index="0x4" bg="{{ blue.hex }}"/>
  <color index="0x5" bg="{{ magenta.hex }}"/>
  <color index="0x6" bg="{{ cyan.hex }}"/>
  <color index="0x7" bg="{{ white.hex }}"/>
  <color index="0x8" bg="{{ bright_black.hex }}"/>
  <color index="0x9" bg="{{ bright_red.hex }}"/>
  <color index="0xa" bg="{{ bright_green.hex }}"/>
  <color index="0xb" bg="{{ bright_yellow.hex }}"/>
  <color index="0xc" bg="{{ bright_blue.hex }}"/>
  <color index="0xd" bg="{{ bright_magenta.hex }}"/>
  <color index="0xe" bg="{{ bright_cyan.hex }}"/>
  <color index="0xf" bg="{{ bright_white.hex }}"/>
  <vfs> <dir name="fonts"> <fs label="fonts"/> </dir> </vfs>
</config>
