<config initial_columns="120" initial_rows="36">
  <!-- srcery color scheme -->
  <color index="0x0" bg="{{ primary.black.hex }}"/>
  <color index="0x1" bg="{{ primary.red.hex }}"/>
  <color index="0x2" bg="{{ primary.green.hex }}"/>
  <color index="0x3" bg="{{ primary.yellow.hex }}"/>
  <color index="0x4" bg="{{ primary.blue.hex }}"/>
  <color index="0x5" bg="{{ primary.magenta.hex }}"/>
  <color index="0x6" bg="{{ primary.cyan.hex }}"/>
  <color index="0x7" bg="{{ primary.white.hex }}"/>
  <color index="0x8" bg="{{ primary.bright_black.hex }}"/>
  <color index="0x9" bg="{{ primary.bright_red.hex }}"/>
  <color index="0xa" bg="{{ primary.bright_green.hex }}"/>
  <color index="0xb" bg="{{ primary.bright_yellow.hex }}"/>
  <color index="0xc" bg="{{ primary.bright_blue.hex }}"/>
  <color index="0xd" bg="{{ primary.bright_magenta.hex }}"/>
  <color index="0xe" bg="{{ primary.bright_cyan.hex }}"/>
  <color index="0xf" bg="{{ primary.bright_white.hex }}"/>
  <vfs> <dir name="fonts"> <fs label="fonts"/> </dir> </vfs>
</config>
