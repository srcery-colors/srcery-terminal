#!/bin/sh
if [ "$TERM" = "linux" ]; then
  /bin/echo -e "
  \e]P0{{sub black.hex }}
  \e]P1{{sub red.hex }}
  \e]P2{{sub green.hex }}
  \e]P3{{sub yellow.hex }}
  \e]P4{{sub blue.hex }}
  \e]P5{{sub magenta.hex }}
  \e]P6{{sub cyan.hex }}
  \e]P7{{sub white.hex }}
  \e]P8{{sub bright_black.hex }}
  \e]P9{{sub bright_red.hex }}
  \e]PA{{sub bright_green.hex }}
  \e]PB{{sub bright_yellow.hex }}
  \e]PC{{sub bright_blue.hex }}
  \e]PD{{sub bright_magenta.hex }}
  \e]PE{{sub bright_cyan.hex }}
  \e]PF{{sub bright_white.hex }}
  "
  # get rid of artifacts
  clear
fi
