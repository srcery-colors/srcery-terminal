#!/bin/sh
if [ "$TERM" = "linux" ]; then
  /bin/echo -e "
  \e]P0{{ primary.black.hex }}
  \e]P1{{ primary.red.hex }}
  \e]P2{{ primary.green.hex }}
  \e]P3{{ primary.yellow.hex }}
  \e]P4{{ primary.blue.hex }}
  \e]P5{{ primary.magenta.hex }}
  \e]P6{{ primary.cyan.hex }}
  \e]P7{{ primary.white.hex }}
  \e]P8{{ primary.bright_black.hex }}
  \e]P9{{ primary.bright_red.hex }}
  \e]PA{{ primary.bright_green.hex }}
  \e]PB{{ primary.bright_yellow.hex }}
  \e]PC{{ primary.bright_blue.hex }}
  \e]PD{{ primary.bright_magenta.hex }}
  \e]PE{{ primary.bright_cyan.hex }}
  \e]PF{{ primary.bright_white.hex }}
  "
  # get rid of artifacts
  clear
fi
