t.prefs_.set("color-palette-overrides", [
	"{{ black.hex }}",
	"{{ red.hex }}",
	"{{ green.hex }}",
	"{{ yellow.hex }}",
	"{{ blue.hex }}",
	"{{ magenta.hex }}",
	"{{ cyan.hex }}",
	"{{ white.hex }}",
	"{{ bright_black.hex }}",
	"{{ bright_red.hex }}",
	"{{ bright_green.hex }}",
	"{{ bright_yellow.hex }}",
	"{{ bright_blue.hex }}",
	"{{ bright_magenta.hex }}",
	"{{ bright_cyan.hex }}",
	"{{ bright_white.hex }}"
]);

t.prefs_.set("foreground-color", "{{ bright_white.hex }}");
t.prefs_.set("background-color", "{{ black.hex }}");
t.prefs_.set("cursor-color",     "{{ bright_yellow.hex }}");
